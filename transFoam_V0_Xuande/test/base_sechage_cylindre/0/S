FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      S;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.99;

boundaryField
{
    cylinder
    {
        type            fixedValue;
        value           uniform 0.659;
    }

    bottomLoading
    {
        type            fixedValue;
        value           uniform 0.659;
    }

    topLoading
    {
        type            fixedValue;
        value           uniform 0.659;
    }

}

